<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane styleClass="mainPane" stylesheets="@../style/StyleSheet.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="1000.0">
      <padding>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
      </padding>
      <ImageView fitHeight="255.0" fitWidth="282.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
         <Image url="@../ressource/images/logo.png" />
      </ImageView>
      <Label fx:id="_LabelVictoire" alignment="CENTER" prefHeight="77.0" prefWidth="459.0" text="Label" textAlignment="CENTER">
         <styleClass>
            <String fx:value="mainLabel" />
            <String fx:value="addOpacity" />
         </styleClass>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="_RevenirEcranTitre" mnemonicParsing="false" text="Revenir&#10;à l'écran&#10;titre" textAlignment="CENTER">
         <VBox.margin>
            <Insets top="80.0" />
         </VBox.margin>
      </Button>
   </VBox>
   <Pane fx:id="_NavbarPane" prefHeight="200.0" prefWidth="80.0" />
</Pane>
